
<%- include("_header") -%>

<h1>Modifier une fiche</h1>

<form action="/editQuestio/<%= model.id %>" method="post">

  <div class="form-horizontal">

   <div class="form-group row">
  <label class="col-form-label col-sm-2" for="questionar">Reponse AR </label>
  <div class="col-sm-7">
    <input class="form-control" name="question_ar" value="<%= model.question_ar %>" />
  </div>
</div>
<div class="form-group row">
  <label class="col-form-label col-sm-2" for="auteur">Reponse FR </label>
  <div class="col-sm-7">
    <input class="form-control" name="question_fr" value="<%= model.question_fr %>" />
  </div>
</div>

    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Modifier" class="btn btn-default btn-warning" />
        <a class="btn btn-outline-dark cancel" href="/data">Annuler</a>
      </div>
    </div>

  </div>
</form>

<%- include("_footer") -%>
